
{
   "name": "littleflow-language",
   "displayName": "Littleflow Language",
   "description": "Syntax highlighting and language configuration for littleflow workflows",
   "version": "0.0.2",
   "publisher": "Alex Mi≈Çowski",
   "engines": {
